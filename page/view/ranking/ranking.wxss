/* page/view/ranking/ranking.wxss */
.left {
  display: flex;
  align-items: center;
  width: 90%;
}
.info-header {
  background-color: #FFF;
  width: 100%;
  height: 150rpx;
  display: flex;
}
.userInfo-avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  margin: 0 20rpx;
}
.ranking-info {
  color: #aaa;
  font-size: 26rpx;
}

.right {
  display: flex;
  align-items: center;
}

.ranking-list {
  margin-top: 10rpx;
}
.ranking-li {
  display: flex;
  width: 100%;
  height: 150rpx;
  background-color: #FFF;
  border-bottom: 1rpx solid #E8E8E8;
}
.ranking-left {
  display: flex;
  align-items: center;
  width: 85%;
  padding-left: 5%
}

.ranking-right {
  display: flex;
  align-items: center;
}

.no-tip {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 50%;
}